{
  "name": "sitewhere-admin-ui",
  "productName": "SiteWhere Admin",
  "version": "3.0.0",
  "description": "SiteWhere Administration",
  "main": "index.js",
  "scripts": {
    "dev": "electron-webpack dev",
    "build": "electron-webpack && electron-builder",
    "publish": "electron-webpack && electron-builder --mac --win --linux -p always"
  },
  "build": {
    "appId": "com.sitewhere.admin",
    "publish": [
      {
        "provider": "github",
        "owner": "sitewhere",
        "repo": "sitewhere-admin-ui"
      }
    ],
    "dmg": {
      "icon": null
    },
    "mac": {
      "category": "public.app-category.business"
    },
    "linux": {
      "category": "Monitor"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitewhere/sitewhere-admin-ui.git"
  },
  "keywords": [
    "sitewhere",
    "user",
    "interface",
    "electron"
  ],
  "author": "Derek Adams <derek.adams@sitewhere.com>",
  "license": "CPAL-1.0",
  "bugs": {
    "url": "https://github.com/sitewhere/sitewhere-admin-ui/issues"
  },
  "homepage": "https://github.com/sitewhere/sitewhere-admin-ui#readme",
  "electronWebpack": {
    "whiteListedModules": [
      "vue",
      "vuetify"
    ]
  },
  "devDependencies": {
    "@types/electron-is-dev": "^0.3.0",
    "@types/leaflet": "^1.4.6",
    "@types/leaflet-draw": "^0.4.14",
    "@types/node": "^10.14.12",
    "@types/universal-analytics": "^0.4.2",
    "@types/uuid": "^3.4.5",
    "@types/vuelidate": "^0.7.5",
    "electron": "^4.2.12",
    "electron-builder": "^22.3.5",
    "electron-updater": "^3.2.3",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "electron-webpack-vue": "^2.3.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "sitewhere-admin-ui-plugins": "^3.0.0-alpha.1",
    "sitewhere-ide-components": "3.0.0-alpha.1",
    "tslint": "^5.18.0",
    "typescript": "^3.8.2",
    "vue-template-compiler": "^2.6.11",
    "vue2-ace-editor": "0.0.11",
    "webpack": "^4.35.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@trodi/electron-splashscreen": "^0.3.4",
    "axios": "^0.18.1",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.17",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.2",
    "lodash": "^4.17.15",
    "material-icons": "^0.2.3",
    "moment": "^2.24.0",
    "mqtt": "^2.16.0",
    "sitewhere-configuration-model": "3.0.0-alpha.1",
    "sitewhere-rest-api": "3.0.0-alpha.1",
    "sockjs-client": "^1.1.4",
    "source-map-support": "^0.5.8",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.3.2",
    "vue": "^2.6.11",
    "vue-clipboards": "^1.2.2",
    "vue-color": "^2.7.0",
    "vue-flatpickr-component": "^2.4.1",
    "vue-highlightjs": "^1.3.3",
    "vue-moment": "^3.2.0",
    "vue-router": "^3.0.7",
    "vue-sticky-directive": "0.0.6",
    "vue2-leaflet": "0.0.58",
    "vuedraggable": "^2.18.1",
    "vuelidate": "^0.7.4",
    "vuetify": "^1.5.23",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4",
